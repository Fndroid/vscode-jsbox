{
  "name": "jsbox",
  "displayName": "JSBox",
  "description": "JSBox Development Utilities on VSCode",
  "author": "Cyan <log.e@qq.com>",
  "icon": "asset/icon.png",
  "version": "0.0.3",
  "publisher": "Ying",
  "engines": {
    "vscode": "^1.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cyanzhong/vscode-jsbox"
  },
  "keywords": [
    "JSBox",
    "jsbox",
    "xTeko",
    "xteko"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onDebug",
    "onLanguage:javascript"
  ],
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.5.3",
    "vscode": "^1.1.5",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "request": "^2.79.0"
  },
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "JSBox configuration",
      "properties": {
        "jsBox.host": {
          "type": "string",
          "default": "",
          "description": "Fill this with your connect host."
        }
      }
    },
    "commands": [
      {
        "command": "jsBox.setHost",
        "title": "JSBox: Set Host"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "jsBox.setHost"
        }
      ]
    },
    "grammars": [
      {
        "scopeName": "source.jsbox",
        "path": "./syntaxes/jsbox.json",
        "injectTo": [
          "source.js",
          "source.ts"
        ]
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/jsbox.json"
      }
    ]
  }
}
